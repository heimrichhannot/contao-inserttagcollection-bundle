{
    "name": "heimrichhannot/contao-inserttagcollection-bundle",
    "description": "A collection of additional inserttags for contao cms.",
    "type": "contao-bundle",
    "homepage":"https://github.com/heimrichhannot/contao-inserttagcollection-bundle",
    "authors":[
        {
            "name":"Heimrich & Hannot",
            "email":"digitales@heimrich-hannot.de",
            "homepage":"https://www.heimrich-hannot.de",
            "role":"Developer"
        }
    ],
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": "^7.1",
        "ext-dom": "*",
        "contao/core-bundle": "^4.4",
        "heimrichhannot/contao-utils-bundle": "^2.25"
    },
    "require-dev": {
        "contao/test-case": "1.1.*",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^3.2",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\ContaoInserttagCollectionBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HeimrichHannot\\ContaoInserttagCollectionBundle\\Test\\": "tests/"
        }
    },
    "replace": {
        "heimrichhannot/contao-inserttags_absolute": "*",
        "heimrichhannot/contao-inserttag_email": "*",
        "heimrichhannot/contao-inserttag_download": "*"
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\ContaoInserttagCollectionBundle\\ContaoManager\\Plugin"
    }
}
